const Main = input => {
  input = input.split("\n");
  let n = +input[0];
  let a = input[1].split(" ").map(Number);

  let ans = 0;
  for (let i = 0; i <= n; i++) {
    if (!a.includes(i)) {
      ans = i;
      console.log(ans);
      return;
    }
  }

}
Main(require("fs").readFileSync("/dev/stdin", "utf8").trim());
