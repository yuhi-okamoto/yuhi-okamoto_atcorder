const Main = input =>{
    const args = input.split("\n");
    const nums = args.map((n) => n.split(" "));
    const x = nums.map((n) => parseInt(n[0], 10));
    const y = nums.map((n) => parseInt(n[1], 10));

    let R = x[0];
    let C = y[0];
  
  	if(C === 1){
      console.log(x[R]);
    }else{
      console.log(y[R]);
    }
};

Main(require("fs").readFileSync("/dev/stdin", "utf8"));
