function Main(input) {
  const args = input.split('\n');
  const nums = args[0].split(' ');
  const a = parseInt(nums[0], 10);
  const b = parseInt(nums[1], 10);
  let c = Math.sqrt(a**2 + b**2);
  let x = a/c;
  let y = b/c;
  console.log(x);
  console.log(y);
}
Main(require("fs").readFileSync("/dev/stdin", "utf8"));
