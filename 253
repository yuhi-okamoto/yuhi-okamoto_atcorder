'use strict'
function main(input) {
  input=input.split(" ");
  const A = parseInt(input[0]);
  const B = parseInt(input[1]);
  const C = parseInt(input[2]);
  if(A>=B && B>=C || A<=B && B<=C){
    console.log("Yes")
  }
  else{
    console.log("No");
  }
}
main(require('fs').readFileSync('/dev/stdin', 'utf8'));
