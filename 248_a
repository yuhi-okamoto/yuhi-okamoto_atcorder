const main = input =>{
  const args = input.split('\n');
  const nums = args[0].split(' ');
  const a = Array.from(nums[0]);  // 入力
  const b = a.sort();
  for(let i=0;i<10;i++){
  	if(b[i] != i){
    	console.log(i);
      	break;
    }
  }
};

main(require("fs").readFileSync("/dev/stdin", "utf8"));
